#include<stdio.h>
#include<conio.h>

int main()
{
int x,y;
printf("MENGETAHUI KUADRAN DARI INPUTAN KOORDINAT X DAN Y\n");
printf("=================================================\n");

printf("Masukkan nilai x : ");
scanf("%i",&x);
printf("Masukkan nilai y : ");
scanf("%i",&y);

if(x>0&&y>0)
printf("KUADRAN 1");

if(x>0&&y>0)
printf("KUADRAN 2");

if(x>0&&y>0)
printf("KUADRAN 3");

if(x>0&&y>0)
printf("KUADRAN 4");

if(x==0&&y==0)
printf("TITIK PUSAT");

getch();
}

